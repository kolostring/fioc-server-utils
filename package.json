{
  "name": "@fioc/next",
  "version": "1.2.0",
  "type": "module",
  "license": "MIT",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "keywords": [
    "react",
    "nextjs",
    "server components",
    "server actions",
    "dependency injection",
    "functional dependency injection",
    "inversion of control",
    "di container",
    "typescript",
    "type-safe",
    "fioc"
  ],
  "description": "A lightweight, type-safe utility library for @fioc/core, optimized for Next.js Server Components and Server Actions, enabling seamless dependency injection in modern React applications.",
  "author": "kolostring <yoerodriguezjunior@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kolostring/fioc-next.git"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "release": "tsup && npm publish --access public"
  },
  "dependencies": {
    "@fioc/core": "^1.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.3.1",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "tsup": "^8.5.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "next": "^14.0.0"
  }
}
